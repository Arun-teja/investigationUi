{
    "_version": "1.26.0",
    "sap.app": {
        "id": "com.pr.prInvestigation",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "mService": {
				"uri": "v2/productrecall/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"sapService": {
				"uri": "sap/opu/odata/LTIPR/ZLS_PROD_RECALL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
        },
        "crossNavigation": {
            "inbounds": {
                "investigation-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "pr_investigation",
                    "action": "create",
                    "title": "Create Investigation",
                    "subTitle": "",
                    "icon": "sap-icon://add"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.pr.prInvestigation.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "pModel":{
                "dataSource": "mService",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None",
					"preload": true
				}
            },
            "prodRecall": {
				"dataSource": "sapService",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None",
					"preload": true
				}
			}
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.pr.prInvestigation.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "Routeinvestigation",
                    "pattern": "Routeinvestigation",
                    "target": [
                        "Targetinvestigation"
                    ]
                }
            ],
            "targets": {
                "Targetinvestigation": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "investigation",
                    "viewName": "investigation"
                }
            }
        },
        "rootView": {
            "viewName": "com.pr.prInvestigation.view.investigation",
            "type": "XML",
            "async": true,
            "id": "investigation"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "Precall"
    }
}